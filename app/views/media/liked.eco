<% for medium in @media: %>
  <div id="<%= "media-liked-#{medium.id}" %>" class="media" >
    <img src="<%= medium.images.standard_resolution.url %>"/>
    <div class="media-detail">
      <div class="user"/>
        <%- (require 'views/partials/user')(medium.user) %>
      </div>
    </div>
  </div>
<% end %>
